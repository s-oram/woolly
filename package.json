{
  "name": "go-to-code",
  "displayName": "Go To Code",
  "description": "Shortcuts to go to the code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-to-code.openStories",
        "title": "GoToCode: Open Stories"
      },
      {
        "command": "go-to-code.openTests",
        "title": "GoToCode: Open Tests"
      },
      {
        "command": "go-to-code.openStyles",
        "title": "GoToCode: Open Styles"
      },
      {
        "command": "go-to-code.openSource",
        "title": "GoToCode: Open Source"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "build-vsix": "mkdir -p extension && vsce package --out 'extension/go-to-code.vsix' --allow-missing-repository --skip-license",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "jest --watch"
  },
  "dependencies": {
    "text-case": "^1.2.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@types/jest": "^30.0.0",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/vsce": "^3.6.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "packageManager": "npm@11.6.2+sha512.ee22b335fcbc95662cdf3ab8a053daf045d9cf9c6df6040d28965abb707512b2c16fa6c5eec049d34c74f78f390cebd14f697919eadb97756564d4f9eccc4954"
}
